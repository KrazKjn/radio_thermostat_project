{
    "expo": {
      "name": "My Thermostat App",
      "displayName": "My Thermostat App",
      "slug": "my-thermostat-app",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.webp",
      "splash": {
        "image": "./assets/thermostat_01.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.mythermostat.app"
      },
      "android": {
        "package": "com.mythermostat.app",
        "permissions": [],
        "adaptiveIcon": {
          "foregroundImage": "./assets/thermostat_02.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.jpg"
      }
    }
}